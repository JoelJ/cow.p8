pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- castle of wonder

b = {}
b[""] = 0
b[""] = 1
b[""] = 2
b[""] = 3
b[""] = 4
b[""] = 5

tick = 0

screen = {}
player = {}
enemies = {}

function _init()
	screen.x = 0
	screen.y = 0

	player.moved = false
	player.x = 7
	player.y = 1
	player.sprite = 0
	player.direction = b[""]

	enemies.count = 1
	for i = 0, enemies.count do
		enemies[i] = {}
		enemies[i].x = 1
		enemies[i].y = 7
		enemies[i].sprite = 4
		enemies[i].direction = b[""]
		enemies[i].tick = 10
	end
end

function _update()
	if not player.moved and
    btn(b[""]) then 
 	if	is_pushable_at(player.x-1, player.y, player.x-2, player.y) then
	 	swap_tiles(player.x-1, player.y, player.x-2, player.y)
		end

  if not is_solid_at(player.x - 1, player.y) then
	  player.x = player.x - 1
		end
		
	 player.moved = true
	 player.direction = b[""]
	end
	
	if not player.moved and
    btn(b[""]) then 
 	if	is_pushable_at(player.x+1, player.y, player.x+2, player.y) then
	 	swap_tiles(player.x+1, player.y, player.x+2, player.y)
		end

  if not is_solid_at(player.x + 1, player.y) then
	  player.x = player.x + 1
	 end
	
	 player.moved = true
	 player.direction = b[""]
	end
	
	if not player.moved and
    btn(b[""]) then
 	if	is_pushable_at(player.x, player.y+1, player.x, player.y+2) then
	 	swap_tiles(player.x, player.y+1, player.x, player.y+2)
		end
    
		if not is_solid_at(player.x, player.y + 1) then
	  player.y = player.y + 1
		end
  
	 player.moved = true
	 player.direction = b[""]
	end
	
	if not player.moved and
    btn(b[""]) then
  if	is_pushable_at(player.x, player.y - 1, player.x, player.y-2) then
	 	swap_tiles(player.x, player.y-1, player.x, player.y-2)
		end
  if not is_solid_at(player.x, player.y - 1) then
	  player.y = player.y - 1
		end

	 player.moved = true
	 player.direction = b[""]
	end
	
	if not btn(b[""]) and
			 not btn(b[""]) and
			 not btn(b[""]) and
			 not btn(b[""]) then
		player.moved = false
	end
	
	if btn(b[""]) then 
	end
	
	if btn(b[""]) then
	end
	
	if player.x >= 16 then
		player.x = 0
		screen.x = screen.x + 1
	end
	
	if player.x < 0 then
		player.x = 15
		screen.x = screen.x - 1
	end
	
	if player.y >= 14 then
		player.y = 0
		screen.y = screen.y + 1
	end
	
	if player.y < 0 then
		player.y = 13
		screen.y = screen.y - 1
	end
	-- should be last line of func
	tick = tick + 1
	if tick > 99 then
		tick = tick - flr(tick / 99) * 99
	end
end

function _draw()
	cls()
	
	map(screen.x * 16,screen.y * 14, 0,0,16,14)
	spr(
		player.sprite + player.direction, 
		player.x * 8, player.y * 8
	)
	
	print("t:"..tick, 0, 123, 8)
	print("px " .. player.x .. " py" .. player.y, 20, 123, 8)
	print("tl "..get_tile_at(player.x, player.y), 0, 117, 8)
	print("srn "..screen.x..","..screen.y, 24, 117, 8)
end

function get_tile_at(x, y)
	tile_x = x + (16 * screen.x)
	tile_y = y + (14 * screen.y)
	
	return mget(tile_x, tile_y)
end

function is_solid_at(x, y)
	return is_solid(get_tile_at(x,y))
end

function is_solid(tile_num)
	return fget(tile_num, 1)
end

function is_pushable_at(x,y, dx, dy)
	if is_solid_at(dx, dy) then
		return false
	end
	
	tile = get_tile_at(x,y)
	return is_pushable(tile)
end

function is_pushable(tile_num)
	return fget(tile_num, 3)
end

function swap_tiles(x1,y1, x2,y2)
	tile1 = get_tile_at(x1,y1)
	tile2 = get_tile_at(x2,y2)

	mset(
		x1 + (16 * screen.x),
		y1 + (16 * screen.y),
		tile2
	)

	mset(
		x2 + (16 * screen.x),
		y2 + (16 * screen.y),
		tile1
	)
end
